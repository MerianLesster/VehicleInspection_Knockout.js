@{
    ViewBag.Title = "Vehicle Inspection";
}

<script src="~/Scripts/knockout-3.5.1.js"></script>
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/KnockoutModels/VehicleViewModel.js"></script>

<div class="jumbotron jumbotron-fluid">
    <div class="container">
        <h1 class="display-4 font-weight-bold">Vehicle Inspection</h1>
        <hr class="my-4" />
        <p class="lead">
            Vehicle Inspection Web Application.
        </p>
    </div>
</div>

<div class="row">
    <div class="col-md-4">
        <div class="panel-body" data-bind="with: showVehicleViewModel">
            @Html.Partial("Vehicle")
        </div>
    </div>
</div>

<div >
    <div >
        <div class="panel-body" data-bind="with: showInspectionViewModel">
            @Html.Partial("Inspection")
        </div>
    </div>
</div>