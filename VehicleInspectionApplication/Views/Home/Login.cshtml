<div class="panel panel-default">
    <div class="panel-heading">Login</div>
    <div class="panel-body">
        <form role="form" style="padding: 30px">
            <div class="form-group row">
                <label for="name">Username:</label>
                <input id="name" placeholder="Username" type="text" class="form-control" data-bind="value: Username" />
            </div>

            <div class="form-group row">
                <label for="password">Password:</label>
                <input id="password" placeholder="Password" type="password" class="form-control" data-bind="value: Password" />
            </div>

            <div style="float: right" data-bind="hidden: InspectionReady" class="row">
                <input type="button" class="btn btn-primary" value="Login" data-bind="click: signInUser" />
            </div>

        </form>
    </div>
</div>
